<?xml version="1.0" encoding="utf-8" ?>
<!--
	Most basic example of inserting image into panorama. Allowed formats are *.jpg *.gif and *.png
	Setting "handCursor" to "false" couses that cursor is not changed when hovers over hotspot.
	Hotspot id has to be unique among all hotspots in all panoramas.
-->
<SaladoPlayer>
	<global debug="true">
		<control autorotation="enabled:false"/>
	</global>
	<panoramas>
		<panorama id="GreenChecker" path="panoramas/dz_green_check/green_check_f.xml">
			<image id="greenArrow" path="hotspots/images/green_arrow.png" handCursor="false"/>
		</panorama>
	</panoramas>
</SaladoPlayer>