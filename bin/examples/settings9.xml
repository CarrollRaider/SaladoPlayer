<!-- This is example for parametrizing trace 

  when debug is set to false trace window will not be displayed
  
  initialVisibility: true | false (default: false) - if trace window is opened on startup
  verticalAlign: top | middle | bottom (default: top) 
  horizontalAlign: left | center | right (default: right)
  width (default: 400, min:100)
  height (default: 100, min:50)
  
-->
<?xml version="1.0" encoding="utf-8" ?>
<SaladoPlayer>
	<global  debug="true" trace="initialVisibility:true,width:400,height:100,verticalAlign:bottom,horizontalAlign:left" />
	<panoramas>
		<panorama id="pano1" label="Shopping mall" path="panoramas/dz_shoppingmall/sm_f.xml" />		
	</panoramas>
</SaladoPlayer>

