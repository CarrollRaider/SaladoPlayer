<!--
	
	branding visibility
	
	- branding is visible by default
	- in order to hide branding set visible to false (show support to project and don't hide branding)
	- you can set branding transparency with alpha attribute
	- you can change branding position with move and align attributes
	- see: http://panozona.com/wiki/Module:NavigationBar#branding
	
-->
<?xml version="1.0" encoding="utf-8"?>
<SaladoPlayer>
	<global debug="true"/>
	<panoramas>
		<panorama id="pano1" label="Park" path="panoramas/dz_park/park_f.xml"/>
		<panorama id="pano2" label="Shopping mall" path="panoramas/dz_shoppingmall/sm_f.xml"/>
		<panorama id="pano3" label="Volleyball" path="panoramas/dz_volleyball/vb_f.xml"/>
	</panoramas>
	<modules>
		<NavigationBar path="modules/NavigationBar.swf">
			<buttons path="modules/images/buttons_30x30_green.png"/>
			<branding visible="false"/>
		</NavigationBar>
	</modules>
</SaladoPlayer>