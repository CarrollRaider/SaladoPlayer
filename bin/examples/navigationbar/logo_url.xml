<!--
	
	logo url target
	
	- url is optional, it determines what site to open when logo is clicked
	- url value is stored in text node in CDATA
	- you can alter logo position with align and move attributes
	- see: http://panozona.com/wiki/Module:NavigationBar#logo
-->
<?xml version="1.0" encoding="utf-8"?>
<SaladoPlayer>
	<global debug="true"/>
	<panoramas>
		<panorama id="pano1" label="Park" path="panoramas/dz_park/park_f.xml"/>
		<panorama id="pano2" label="Shopping mall" path="panoramas/dz_shoppingmall/sm_f.xml"/>
		<panorama id="pano3" label="Volleyball" path="panoramas/dz_volleyball/vb_f.xml"/>
	</panoramas>
	<modules>
		<NavigationBar path="modules/NavigationBar.swf">
			<buttons path="modules/images/buttons_30x30_green.png"/>
			<logo visible="true" path="modules/images/logo.png" align="horizontal:left,vertical:bottom" move="vertical:-100">
				<![CDATA[http://panozona.com/]]>
			</logo>
		</NavigationBar>
	</modules>
</SaladoPlayer>